Import('env')
env = env.Clone()
env.Append(CPPPATH = ".")
env.Append(SHLINKFLAGS = "-Wl,-undefined,dynamic_lookup")
env.SetDefault(SWIGMODULE = 'FirstTest')
swigout = env.GoSwigComplete('swig1', 'FirstTest')
lib = env.SharedLibrary(env.subst('$SWIGMODULE'), ['foo.cpp', swigout[0]])
outfile = env.subst('#/$OUTDIR/') + str(lib[0])
env.Install('#$OUTDIR', lib)
